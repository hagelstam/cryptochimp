    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tremor/react": "^3.18.7",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "next-auth": "4.24.13",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@types/bun": "^1.3.5",
